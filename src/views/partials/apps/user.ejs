<% var url = "/u/" + data.hash.toLowerCase() %>
<app-profile tab="<%= data.tab %>" you="<%= data.you ? 'true' : 'false' %>" hash="<%= data.hash %>" url="<%= url %>" stories-url="/api/v1<%= url %>/stories" 
  replies-url="/api/v1<%= url %>/replies" stories="<%= data.stories %>" replies="<%= data.replies %>" 
  followers-url="/api/v1<%= url %>/followers" following-url="/api/v1<%= url %>/following" contact="<%= data.contact %>"
  picture="<%= data.picture %>" verified="<%= data.verified ? 'true' : 'false' %>" name="<%= data.name %>" followers="<%= data.followers %>"
  following="<%= data.following %>" user-follow="<%= data.is_following ? 'true' : 'false' %>" bio="<%= data.bio === null ? 'This user has not added a bio yet.' : data.bio %>">
</app-profile>